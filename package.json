{
  "name": "yarn-monorepo",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "workspace-a",
    "workspace-b",
    "packages/*"
  ],
  "scripts": {
    "workspace-a": "yarn --cwd workspace-a build",
    "workspace-a:dev": "yarn --cwd workspace-a dev",
    "workspace-b": "yarn --cwd workspace-b build",
    "workspace-b:dev": "yarn --cwd workspace-b dev"
  }
}
